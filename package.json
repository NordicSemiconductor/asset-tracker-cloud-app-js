    "history": "5.3.0",
    "intro.js": "6.0.0",
    "io-ts": "2.2.20",
    "io-ts-types": "0.5.19",
    "leaflet": "1.9.3",
    "lodash": "4.17.21",
    "mcc-mnc-list": "1.1.10",
    "memoize-one": "6.0.0",
    "msal": "1.4.17",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-leaflet": "3.1.0",
    "react-router-dom": "5.3.4",
    "reactstrap": "8.10.1",
    "styled-components": "5.3.8",
    "twemoji": "14.0.2",
    "use-debounce": "9.0.3",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "7.21.0",
