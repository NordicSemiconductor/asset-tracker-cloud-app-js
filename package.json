    "@microsoft/signalr": "3.1.9",
    "@react-firebase/auth": "0.2.10",
    "aws-amplify": "3.3.4",
    "aws-amplify-react": "4.2.8",
    "aws-iot-device-sdk": "2.2.6",
    "aws-sdk": "2.773.0",
    "azure-iothub": "1.13.0",
    "date-fns": "2.16.1",
    "deepmerge": "4.2.2",
    "e118-iin-list": "1.7.0",
    "fast-deep-equal": "3.1.3",
    "firebase": "7.24.0",
    "fp-ts": "2.8.4",
    "history": "4.10.1",
    "intro.js": "2.9.3",
    "io-ts": "2.2.11",
    "io-ts-types": "0.5.11",
    "leaflet": "1.7.1",
    "mcc-mnc-list": "1.1.3",
    "memoize-one": "5.1.1",
    "msal": "1.4.1",
    "p-queue": "6.6.2",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-leaflet": "2.7.0",
    "react-router-dom": "5.2.0",
    "reactstrap": "8.6.0",
    "snyk": "1.415.0",
    "styled-components": "5.2.0",
    "twemoji": "13.0.1",
    "use-debounce": "5.0.1",
    "uuid": "8.3.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "7.12.1",
    "@bifravst/code-style": "8.0.134",
    "@bifravst/eslint-config-typescript": "5.0.67",
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-angular": "11.0.0",
    "@types/aws-iot-device-sdk": "2.2.2",
    "@types/intro.js": "2.4.7",
    "@types/jest": "26.0.14",
    "@types/node": "14.11.10",
    "@types/react": "16.9.53",
    "@types/react-dom": "16.9.8",
    "@types/react-leaflet": "2.5.2",
    "@types/react-router-dom": "5.1.6",
    "@types/reactstrap": "8.5.1",
    "@types/semver": "7.3.4",
    "@types/styled-components": "5.1.4",
    "@types/twemoji": "12.1.1",
    "@types/uuid": "8.3.0",
    "@typescript-eslint/eslint-plugin": "4.4.1",
    "@typescript-eslint/parser": "4.4.1",
    "babel-eslint": "10.1.0",
    "eslint": "7.11.0",
    "eslint-config-react-app": "5.2.1",
    "eslint-plugin-flowtype": "5.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-react": "7.21.4",
    "eslint-plugin-react-hooks": "4.1.2",
    "husky": "4.3.0",
    "lint-staged": "^10.4.2",
    "prettier": "^2.1.2",
    "react-scripts": "3.4.3",
    "ts-jest": "26.4.1",
    "typescript": "4.0.3"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged && npm test"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --ext .js,.ts,.tsx"
    ],
    "*.{md,json,yaml,yml,html}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=v14.8.0",
    "npm": ">=6.0.0"
  },
  "release": {
    "branches": [
      "saga"
    ],
    "remoteTags": true,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github"
    ]
  },
  "browserslist": [
    ">3%",
    "last 1 version",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "testURL": "http://localhost",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": ".+\\.spec\\.ts$",
    "globals": {
      "ts-jest": {
        "diagnostics": true
      }
    }
  },
  "babelMacros": {
    "styledComponents": {
      "pure": true
    }
  },
  "snyk": true
}
