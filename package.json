{
  "name": "@nordicsemiconductor/asset-tracker-cloud-app",
  "version": "0.0.0-development",
  "description": "The nRF Asset Tracker Web Application is a reference single-page application (SPA) developed using create-react-app in TypeScript.",
  "scripts": {
    "start": "PUBLIC_URL=/ react-scripts --max_old_space_size=4096 start",
    "build": "react-scripts --max_old_space_size=4096 build",
    "test": "jest",
    "eject": "react-scripts eject",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NordicSemiconductor/asset-tracker-cloud-app-js.git"
  },
  "bugs": {
    "url": "https://github.com/NordicSemiconductor/asset-tracker-cloud-app-js/issues"
  },
  "homepage": "https://github.com/NordicSemiconductor/asset-tracker-cloud-app-js#readme",
  "keywords": [
    "nrf-asset-tracker",
    "iot",
    "web-app"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-3-Clause",
  "dependencies": {
    "@amcharts/amcharts4": "4.10.27",
    "@microsoft/signalr": "6.0.8",
    "@nordicsemiconductor/cell-geolocation-helpers": "5.0.1",
    "@nordicsemiconductor/rsrp-bar": "5.0.4",
    "@nordicsemiconductor/timestream-helpers": "4.1.2",
    "@react-leaflet/core": "1.0.2",
    "azure-iothub": "1.15.0",
    "date-fns": "2.29.2",
    "deepmerge": "4.2.2",
    "e118-iin-list": "2.1.1",
    "fast-deep-equal": "3.1.3",
    "fp-ts": "2.12.3",
    "history": "5.3.0",
    "intro.js": "6.0.0",
    "io-ts": "2.2.18",
    "io-ts-types": "0.5.16",
    "leaflet": "1.8.0",
    "lodash": "4.17.21",
    "mcc-mnc-list": "1.1.8",
    "memoize-one": "6.0.0",
    "msal": "1.4.17",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-leaflet": "3.1.0",
    "react-router-dom": "5.3.3",
    "reactstrap": "8.10.1",
    "styled-components": "5.3.5",
    "twemoji": "13.1.0",
    "use-debounce": "8.0.4",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "7.18.9",
    "@nordicsemiconductor/asset-tracker-cloud-code-style": "11.0.121",
    "@swc/jest": "0.2.22",
    "@types/intro.js": "5.1.0",
    "@types/jest": "29.0.0",
    "@types/lodash": "4.14.184",
    "@types/node": "17.0.9",
    "@types/react": "18.0.18",
    "@types/react-dom": "18.0.6",
    "@types/react-leaflet": "2.8.2",
    "@types/react-router-dom": "5.3.3",
    "@types/semver": "7.3.12",
    "@types/styled-components": "5.1.26",
    "@types/twemoji": "12.1.2",
    "@types/uuid": "8.3.4",
    "eslint-config-react-app": "7.0.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-react": "7.31.6",
    "eslint-plugin-react-hooks": "4.6.0",
    "jest": "29.0.2",
    "react-scripts": "4.0.3"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --ext .js,.ts,.tsx"
    ],
    "*.{md,json,yaml,yml,html}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  },
  "release": {
    "branches": [
      "saga"
    ],
    "remoteTags": true,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github"
    ]
  },
  "browserslist": [
    ">3%",
    "last 1 version",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "@swc/jest"
      ]
    }
  },
  "babelMacros": {
    "styledComponents": {
      "pure": true
    }
  }
}
